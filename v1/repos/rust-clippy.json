{
  "org": "rust-lang",
  "name": "rust-clippy",
  "description": "A bunch of lints to catch common mistakes and improve your Rust code. Book: https://doc.rust-lang.org/clippy/",
  "homepage": "https://rust-lang.github.io/rust-clippy/",
  "bots": [
    "bors",
    "rustbot"
  ],
  "teams": [
    {
      "name": "clippy",
      "permission": "write"
    },
    {
      "name": "clippy-contributors",
      "permission": "triage"
    }
  ],
  "members": [],
  "branch_protections": [
    {
      "pattern": "master",
      "ci_checks": [],
      "dismiss_stale_review": false,
      "required_approvals": 1,
      "allowed_merge_teams": []
    }
  ],
  "archived": false,
  "auto_merge_enabled": false
}